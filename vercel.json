{
  "version": 2,
  "builds": [
    {
      "src": "dist/src/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api-docs",
      "dest": "dist/src/main.js"
    },
    {
      "src": "/api-docs/(.*)",
      "dest": "dist/src/main.js"
    },
    {
      "src": "/(.*)",
      "dest": "dist/src/main.js"
    }
  ],
  "env": {
    "PORT": "3000"
  }
} 